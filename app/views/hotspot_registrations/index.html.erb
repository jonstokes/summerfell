<p id="notice"><%= notice %></p>

<h1>Listing Hotspot Registrations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hotspot_registrations.each do |hotspot_registration| %>
      <tr>
        <td><%= link_to 'Show', hotspot_registration %></td>
        <td><%= link_to 'Edit', edit_hotspot_registration_path(hotspot_registration) %></td>
        <td><%= link_to 'Destroy', hotspot_registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hotspot registration', new_hotspot_registration_path %>
