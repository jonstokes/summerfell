<p id="notice"><%= notice %></p>

<h1>Listing Admin Inventory Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_inventory_items.each do |admin_inventory_item| %>
      <tr>
        <td><%= link_to 'Show', admin_inventory_item %></td>
        <td><%= link_to 'Edit', edit_admin_inventory_item_path(admin_inventory_item) %></td>
        <td><%= link_to 'Destroy', admin_inventory_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventory item', new_admin_inventory_item_path %>
